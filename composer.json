{
	"name": "xiewulong/yii2-xui",
	"description": "UI widgets and assets of yii2 extension",
	"keywords": ["yii2", "assets", "widgets", "extension", "ui"],
	"type": "yii2-extension",
	"license": "MIT",
	"support": {
		"issues": "https://github.com/xiewulong/yii2-xui/issues",
		"wiki": "https://github.com/xiewulong/yii2-xui/wiki",
		"source": "https://github.com/xiewulong/yii2-xui"
	},
	"authors": [
	{
		"name": "xiewulong",
		"email": "xiewulong@vip.qq.com"
	}
	],
	"require": {
		"yiisoft/yii2": "*",
		"bower-asset/bootstrap": "*",
		"bower-asset/font-awesome": "*",
		"bower-asset/html5shiv": "*",
		"bower-asset/jquery": "*",
		"bower-asset/react": "*",
		"bower-asset/respond-minmax": "*",
		"bower-asset/smalot-bootstrap-datetimepicker": "*",
		"bower-asset/weui": "*",
		"npm-asset/axios": "*",
		"npm-asset/vue": "*",
		"npm-asset/vue-router": "*",
		"npm-asset/vuex": "*",
		"tinymce/tinymce": "*",
		"xiewulong/jqueryx": "*",
		"xiewulong/samples": "*"
	},
	"extra": {
		"asset-installer-paths": {
			"bower-asset-library": "vendor/bower",
			"npm-asset-library": "vendor/npm"
		}
	},
	"autoload": {
		"psr-4": {"yii\\xui\\": ""}
	}
}
